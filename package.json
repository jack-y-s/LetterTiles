{
  "name": "letter-tiles",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm --workspace backend run dev\" \"npm --workspace frontend run dev\"",
    "build": "npm --workspace frontend run build",
    "start": "npm --workspace backend run start",
    "postinstall": "node ./scripts/postinstall-patch.js"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "png-to-ico": "^3.0.1",
    "sharp": "^0.34.5"
  }
}
